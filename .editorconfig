# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[{*.json,*.yml}]
indent_size = 2
indent_style = space

[{*.md,*.markdown}]
trim_trailing_whitespace = false
max_line_length = off

